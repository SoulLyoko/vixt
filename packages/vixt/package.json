{
  "name": "vixt",
  "type": "module",
  "version": "0.5.4",
  "author": "SoulLyoko<https://github.com/SoulLyoko>",
  "license": "MIT",
  "homepage": "https://soullyoko.github.io/vixt/",
  "repository": "https://github.com/SoulLyoko/vixt.git",
  "keywords": [
    "vixt",
    "vite",
    "nuxt",
    "vue",
    "uni",
    "vitepress",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs"
    },
    "./uni": {
      "types": "./dist/uni.d.ts",
      "import": "./dist/uni.mjs"
    },
    "./vitepress": {
      "types": "./dist/vitepress.d.ts",
      "import": "./dist/vitepress.mjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs"
    }
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "dependencies": {
    "@vixt/core": "workspace:*",
    "@vixt/react": "workspace:*",
    "@vixt/uni": "workspace:*",
    "@vixt/vitepress": "workspace:*",
    "@vixt/vue": "workspace:*"
  }
}
